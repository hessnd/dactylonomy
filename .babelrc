{
  "presets": [
    [
      "env",
      {
        "useBuiltIns": "entry",
      }
    ],
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "styled-jsx-plugin-postcss",
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-assets-import-to-string",
      {
        "baseDir": "static",
        "baseUri": "/"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "static": "./static"
        }
      }
    ]
  ]
}
